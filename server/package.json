{
  "name": "frourio-app-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run migrate:dev && run-p dev:*",
    "dev:server": "webpack --watch --mode=development",
    "dev:frourio": "frourio-express --watch",
    "dev:prisma": "prisma generate --watch",
    "prebuild": "npm run generate",
    "build": "webpack --mode=production",
    "generate": "run-p generate:*",
    "generate:frourio": "frourio-express",
    "generate:prisma": "prisma generate",
    "migrate:dev": "prisma migrate dev",
    "migrate:dev:createonly": "prisma migrate dev --create-only",
    "migrate:reset": "prisma migrate reset",
    "pm2:start": "pm2 start pm2.config.json --env production",
    "pm2:stop": "pm2 stop pm2.config.json",
    "pm2:delete": "pm2 delete pm2.config.json",
    "pm2:logs": "pm2 logs",
    "pm2:monit": "pm2 monit",
    "start": "cross-env NODE_ENV=production node index.js"
  },
  "dependencies": {
    "@prisma/client": "^2.21.2",
    "class-validator": "^0.13.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.1.0",
    "helmet": "^4.5.0",
    "multer": "^1.4.2",
    "pm2": "^4.5.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express-jwt": "^6.0.2",
    "@types/jest": "^26.0.22",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/multer": "^1.4.5",
    "cross-env": "^7.0.3",
    "fastify": "^3.15.0",
    "fastify-cors": "^5.2.0",
    "frourio-express": "^0.25.1",
    "nodemon-webpack-plugin": "^4.5.2",
    "npm-run-all": "^4.1.5",
    "prisma": "^2.21.2",
    "ts-loader": "^8.1.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "~4.2",
    "webpack": "^5.35.0",
    "webpack-cli": "^4.6.0",
    "webpack-node-externals": "^3.0.0"
  }
}
